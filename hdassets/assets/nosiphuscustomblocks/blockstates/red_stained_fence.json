        {   "apply": { "model": "nosiphuscustomblocks:block/red_stained_fence_post" }},
        {   "when": { "north": "true" },
            "apply": { "model": "nosiphuscustomblocks:block/_bdc/fences/red_stained/fence_side_ne" }
